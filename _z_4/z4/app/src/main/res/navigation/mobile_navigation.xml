<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_cat">

    <fragment
        android:id="@+id/navigation_cat"
        android:name="kz.ermolaev.z4.ui.home.HomeFragment"
        android:label="@string/title_cat"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_ext"
        android:name="kz.ermolaev.z4.ui.dashboard.DashboardFragment"
        android:label="@string/title_ext"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_mail"
        android:name="kz.ermolaev.z4.ui.notifications.NotificationsFragment"
        android:label="@string/title_mail"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_webview"
        android:name="kz.ermolaev.z4.navigation_webview"
        android:label="@string/title_cat"
        tools:layout="@layout/navigation_webview" />
</navigation>